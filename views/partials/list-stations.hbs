{{#each stations}}
<div class="box box-link-hover-shadow">
  <h2 class="title">
    <a href="/station/{{_id}}">
      {{name}}
    </a>
    <a href="/station/{{_id}}" class="button">
      <span class="icon is-small">
        {{>icons/open }}
      </span>
    </a>
    <a href="/dashboard/deletestation/{{_id}}" class="button">
      <span class="icon is-small">
        {{>icons/delete }}
      </span>
    </a>
  </h2>
</div>
{{/each}}